{
  "name": "alisher",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "postbuild": "npm run sitemap",
    "start": "next start",
    "lint": "next lint",
    "sitemap": "cross-env NODE_OPTIONS='--experimental-json-modules' node ./src/scripts/generateSitemap.mjs",
    "analyze": "cross-env ANALYZE=true next build",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,mdx,css,json}\"",
    "format:fix": "prettier --write \"src/**/*.{js,jsx,ts,tsx,mdx,css,json}\"",
    "prepare": "husky install"
  },
  "dependencies": {
    "@bprogress/next": "^3.2.12",
    "@headlessui/react": "^2.2.0",
    "@heroicons/react": "^2.2.0",
    "@tailwindcss/forms": "^0.5.9",
    "@vercel/analytics": "^1.4.0",
    "emailjs-com": "^3.2.0",
    "formik": "^2.4.6",
    "framer-motion": "^12.0.0",
    "gsap": "^3.12.6",
    "lru-cache": "^11.0.0",
    "lucide-react": "^0.460.0",
    "nanoid": "^5.0.8",
    "next": "13.4.4",
    "next-seo": "^6.4.0",
    "next-themes": "^0.2.1",
    "nodemailer": "^6.9.7",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "^5.4.0",
    "sharp": "^0.33.5",
    "yup": "^1.5.0"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^14.0.4",
    "@svgr/webpack": "^8.1.0",
    "@types/node": "^20.10.4",
    "@types/nodemailer": "^6.4.14",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@types/uuid": "^10.0.0",
    "autoprefixer": "^10.4.20",
    "cross-env": "^7.0.3",
    "eslint": "8.42.0",
    "eslint-config-next": "13.4.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "globby": "^14.0.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.3.0",
    "postcss": "^8.5.1",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.6.4",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.2"
  },
  "lint-staged": {
    "*.+(js|jsx|ts|tsx)": [
      "eslint --fix"
    ],
    "*.+(js|jsx|ts|tsx|json|css|md|mdx)": [
      "prettier --write"
    ]
  }
}
